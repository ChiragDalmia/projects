{
  "version": 2,
  "builds": [
    { "src": "30-javascript-projects/**/*", "use": "@vercel/static" },
    { "src": "html-css/youTube-clone/**/*", "use": "@vercel/static" },
    { "src": "not-a-raccoon/package.json", "use": "@vercel/react" }
  ],
  "routes": [
    {
      "src": "/30-javascript-projects/(.*)",
      "dest": "/30-javascript-projects/$1"
    },
    {
      "src": "/youtube-clone/(.*)",
      "dest": "/html-css/youTube-clone/$1"
    },
    {
      "src": "/not-a-raccoon/(.*)",
      "dest": "/not-a-raccoon/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/not-a-raccoon/(.*)",
      "dest": "/not-a-raccoon/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/30-javascript-projects/$1"
    }
  ]
}
