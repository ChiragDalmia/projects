{
  "version": 2,
  "builds": [
    { "src": "30-javascript-projects/*/index.html", "use": "@vercel/static" },
    { "src": "html-css/youTube-clone/index.html", "use": "@vercel/static" },
    { "src": "not-a-raccoon/package.json", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/30-javascript-projects/(.*)",
      "dest": "/30-javascript-projects/$1"
    },
    { "src": "/youtube-clone/(.*)", "dest": "/html-css/youTube-clone/$1" },
    { "src": "/(.*)", "dest": "/not-a-raccoon/$1" }
  ]
}
